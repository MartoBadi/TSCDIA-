{
  "name": "app_estudio",
  "version": "1.0.0",
  "description": "App para estudiar.",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "pack:win": "electron-packager . app_estudio --platform=win32 --arch=x64 --out=dist --overwrite",
    "pack:linux": "electron-packager . app_estudio --platform=linux --arch=x64 --out=dist --overwrite",
    "pack:mac": "electron-packager . app_estudio --platform=darwin --arch=x64 --out=dist --overwrite",
    "pack:all": "electron-packager . app_estudio --platform=win32,linux,darwin --arch=x64 --out=dist --overwrite"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^38.2.1",
    "electron-packager": "^17.1.2"
  }
}
